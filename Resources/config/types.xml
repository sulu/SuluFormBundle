<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="sulu_form.dynamic.type_text" class="Sulu\Bundle\FormBundle\Dynamic\Types\Text">
            <tag name="sulu_form.dynamic.type" alias="text"/>
        </service>

        <service id="sulu_form.dynamic.type_textarea" class="Sulu\Bundle\FormBundle\Dynamic\Types\Textarea">
            <tag name="sulu_form.dynamic.type" alias="textarea"/>
        </service>

        <service id="sulu_form.dynamic.type_headline" class="Sulu\Bundle\FormBundle\Dynamic\Types\Headline">
            <tag name="sulu_form.dynamic.type" alias="headline"/>
        </service>

        <service id="sulu_form.dynamic.type_spacer" class="Sulu\Bundle\FormBundle\Dynamic\Types\Spacer">
            <tag name="sulu_form.dynamic.type" alias="spacer"/>
        </service>

        <service id="sulu_form.dynamic.type_freetext" class="Sulu\Bundle\FormBundle\Dynamic\Types\Freetext">
            <tag name="sulu_form.dynamic.type" alias="freetext"/>
        </service>

        <service id="sulu_form.dynamic.type_country" class="Sulu\Bundle\FormBundle\Dynamic\Types\Country">
            <tag name="sulu_form.dynamic.type" alias="country"/>
        </service>

        <service id="sulu_form.dynamic.type_email" class="Sulu\Bundle\FormBundle\Dynamic\Types\Email">
            <tag name="sulu_form.dynamic.type" alias="email"/>
        </service>

        <service id="sulu_form.dynamic.type_date" class="Sulu\Bundle\FormBundle\Dynamic\Types\Date">
            <tag name="sulu_form.dynamic.type" alias="date"/>
        </service>

        <service id="sulu_form.dynamic.type_checkobox" class="Sulu\Bundle\FormBundle\Dynamic\Types\Checkbox">
            <tag name="sulu_form.dynamic.type" alias="checkbox"/>
        </service>

        <service id="sulu_form.dynamic.type_checkboxmultiple"
                 class="Sulu\Bundle\FormBundle\Dynamic\Types\CheckboxMultiple">
            <tag name="sulu_form.dynamic.type" alias="checkboxmultiple"/>
        </service>

        <service id="sulu_form.dynamic.type_dropdown" class="Sulu\Bundle\FormBundle\Dynamic\Types\Dropdown">
            <tag name="sulu_form.dynamic.type" alias="dropdown"/>
        </service>

        <service id="sulu_form.dynamic.type_radiobuttons" class="Sulu\Bundle\FormBundle\Dynamic\Types\Radiobuttons">
            <tag name="sulu_form.dynamic.type" alias="radiobuttons"/>
        </service>

        <service id="sulu_form.dynamic.type_dropdownmultiple"
                 class="Sulu\Bundle\FormBundle\Dynamic\Types\DropdownMultiple">
            <tag name="sulu_form.dynamic.type" alias="dropdownmultiple"/>
        </service>

        <service id="sulu_form.dynamic.type_salutation" class="Sulu\Bundle\FormBundle\Dynamic\Types\Salutation">
            <tag name="sulu_form.dynamic.type" alias="salutation"/>
        </service>

        <service id="sulu_form.dynamic.type_attachment" class="Sulu\Bundle\FormBundle\Dynamic\Types\Attachment">
            <tag name="sulu_form.dynamic.type" alias="salutation"/>
        </service>
    </services>
</container>
